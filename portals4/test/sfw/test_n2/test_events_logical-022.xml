<?xml version="1.0"?>
<test>
      <desc>Test events with get le, md with no eq</desc>
      <ptl>
        <ptl_ni>
          <ompi_rt>
            <ptl_ni ni_opt="NO_MATCH LOGICAL">
              <ptl_eq>
                <ptl_pt>
                  <ptl_get_id>

                    <if rank="0">
                      <ptl_md eq_handle="EQ_NONE">
                        <barrier/>
                        <ptl_get match="0x3f5c" target_id="1" user_ptr="234"/>
                        <barrier/>
                      </ptl_md>
                      <ptl_eq_poll ret="EQ_EMPTY"/>
                    </if>

                    <if rank="1">
                      <ptl_le le_opt="OP_GET EVENT_LINK_DISABLE" uid="ANY" user_ptr="654">
                        <barrier/>
                        <ptl_eq_poll time="200">
                          <check event_type="GET"/>
                          <check event_user_ptr="654"/>
                          <check event_ni_fail="OK"/>
                        </ptl_eq_poll>
                        <ptl_eq_poll ret="EQ_EMPTY"/>
                        <barrier/>
                      </ptl_le>
                    </if>

                  </ptl_get_id>
                </ptl_pt>
              </ptl_eq>
            </ptl_ni>
          </ompi_rt>
        </ptl_ni>
      </ptl>
    </test>