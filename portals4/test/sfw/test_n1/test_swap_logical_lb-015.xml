              </ptl_md>
              <check le_data="0x3da5defc" length="4" type="UINT32"/>
              <check le_data="0x3b44a7aa" length="4" offset="4" type="UINT32"/>
            </ptl_le>
          </ptl_pt>
          <barrier/>
        </ptl_ni>
      </ompi_rt>
    </ptl_ni>
  </ptl>
