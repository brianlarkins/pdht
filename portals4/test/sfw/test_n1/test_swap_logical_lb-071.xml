<?xml version="1.0"?>
<test>
  <desc>Test swap CSWAP_GT/UINT16</desc>
  <ptl>
    <ptl_ni>
      <ompi_rt>
        <ptl_ni ni_opt="NO_MATCH LOGICAL">
          <ptl_pt>
            <ptl_le le_data="0xc2cc" le_opt="OP_PUT OP_GET" type="UINT16">
              <ptl_md md_data="0x142e" type="UINT16">
                <ptl_md md_data="0xa5b2" type="UINT16">
                  <ptl_swap atom_op="CSWAP_GT" atom_type="UINT16" length="2" operand="0xe34e" target_id="SELF"/>
                  <msleep count="50"/>
                </ptl_md>
                <check length="2" md_data="0x142e" type="UINT16"/>
                <check length="2" md_data="0x142e" offset="2" type="UINT16"/>
              </ptl_md>
              <check le_data="0x142e" length="2" type="UINT16"/>
              <check le_data="0xc2cc" length="2" offset="2" type="UINT16"/>
            </ptl_le>
          </ptl_pt>
          <barrier/>
        </ptl_ni>
      </ompi_rt>
    </ptl_ni>
  </ptl>
</test>
