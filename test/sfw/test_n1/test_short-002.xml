<?xml version="1.0"?>
<test>
  <desc>Test short put with offset</desc>
  <ptl>
    <ptl_ni>
      <ptl_pt>
        <ptl_me me_data="0x77" me_match="0x5555" me_opt="OP_PUT">
          <ptl_md md_data="0xaa">
            <ptl_put length="4" match="0x5555" rem_offset="3" target_id="SELF"/>
            <msleep count="50"/>
          </ptl_md>
          <check length="3" me_data="0x77"/>
          <check length="4" me_data="0xaa" offset="3"/>
        </ptl_me>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>
