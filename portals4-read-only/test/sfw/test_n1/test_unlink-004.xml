<?xml version="1.0"?>
<test>
  <desc>Test put me unlink with min_free reached</desc>
  <ptl>
    <ptl_ni ni_opt="MATCH PHYSICAL">
      <ompi_rt>
        <ptl_eq>
          <ptl_pt>
            <ptl_me_append me_length="10" me_match="0x5555" me_min_free="4" me_opt="OP_PUT MANAGE_LOCAL" uid="ANY">
              <ptl_eq>
                <ptl_md>
                  <ptl_put length="7" match="0x5555" target_id="SELF"/>
                  <ptl_eq_wait eq_handle="eq[0]">
                    <check event_type="LINK"/>
                  </ptl_eq_wait>
                  <ptl_eq_wait eq_handle="eq[0]">
                    <check event_type="PUT"/>
                  </ptl_eq_wait>

                  <ptl_eq_wait eq_handle="eq[0]">
                    <check event_type="AUTO_UNLINK"/>
                  </ptl_eq_wait>
                  <ptl_eq_wait eq_handle="eq[1]">
                    <check event_type="SEND"/>
                  </ptl_eq_wait>
                </ptl_md>
                <ptl_me_unlink ret="ARG_INVALID"/>
              </ptl_eq>
            </ptl_me_append>
          </ptl_pt>
        </ptl_eq>
      </ompi_rt>
    </ptl_ni>
  </ptl>
</test>
