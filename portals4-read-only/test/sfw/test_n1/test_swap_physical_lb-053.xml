            <check length="2" md_data="0x9c51" type="UINT16"/>
            <check length="2" md_data="0x9c51" offset="2" type="UINT16"/>
          </ptl_md>
          <check le_data="0x9c51" length="2" type="UINT16"/>
          <check le_data="0xcc51" length="2" offset="2" type="UINT16"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>
