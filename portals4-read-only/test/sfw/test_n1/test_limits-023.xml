<?xml version="1.0"?>
<test>
  <desc>Test max_list_size up to limit for le in overflow</desc>
  <ptl>
    <ptl_ni desired_max_list_size="4" ni_opt="NO_MATCH PHYSICAL">
      <check actual_max_list_size="4"/>
      <ptl_pt>
        <ptl_le count="4" list="OVERFLOW"/>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>
