<?xml version="1.0"?>
<test>
  <desc>Test ct events with put to me, no CT_ACK</desc>
  <ptl>
    <ptl_ni>
      <ompi_rt>
        <ptl_ct>
          <ptl_pt pt_index="2">
            <ptl_me me_ignore="0xff00" me_match="0xa5a5" me_opt="OP_PUT CT_COMM" pt_index="2" uid="ANY">
              <ptl_ct>
                <ptl_md md_opt="CT_SEND">
                  <ptl_put ack_req="CT_ACK" match="0x5aa5" pt_index="2" target_id="SELF"/>
                  <ptl_ct_wait ct_handle="ct[0]" ct_test="1"/>
                  <ptl_ct_get ct_handle="ct[0]">
                    <check ct_event_failure="0" ct_event_success="1"/>
                  </ptl_ct_get>
                  <ptl_ct_wait ct_handle="ct[1]" ct_test="1"/>
                  <ptl_ct_get ct_handle="ct[1]">
                    <check ct_event_failure="0" ct_event_success="1"/>
                  </ptl_ct_get>
                </ptl_md>
              </ptl_ct>
            </ptl_me>
          </ptl_pt>
        </ptl_ct>
      </ompi_rt>
    </ptl_ni>
  </ptl>
</test>
