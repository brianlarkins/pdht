<?xml version="1.0"?>
<test>
      <desc>Test PtlPut with good parameters ME IOVEC of 8*8 </desc>
      <ptl>
        <ptl_ni>
          <ompi_rt>
            <ptl_ni desired_max_msg_size="8192" ni_opt="MATCH LOGICAL">
              <ptl_eq>
                <ptl_pt>
                  <ptl_get_id>

                    <if rank="0">
                      <ptl_md md_data="0x5a5a5a5a" md_length="64" type="UINT32">
                        <barrier/>
                        <ptl_put ack_req="ACK" length="64" match="0x5555" target_id="1"/>
                        <ptl_eq_poll time="200">
                          <check event_type="SEND"/>
                        </ptl_eq_poll>
                        <ptl_eq_poll time="200">
                          <check event_type="ACK"/>
                        </ptl_eq_poll>
                        <barrier/>
                      </ptl_md>
                    </if>

                    <if rank="1">
                      <ptl_me iovec_len="8" me_data="0xa5a5a5a5" me_length="16" me_match="0x5555" me_opt="IOVEC OP_PUT OP_GET EVENT_LINK_DISABLE" type="UINT32">
                        <barrier/>
                        <ptl_eq_poll time="200">
                          <check event_type="PUT"/>
                        </ptl_eq_poll>
                        <check length="64" me_data="0x5a5a5a5a" type="UINT32"/>
                        <check length="64" me_data="0xa5a5a5a5" offset="64" type="UINT32"/>
                        <barrier/>
                      </ptl_me>
                    </if>

                  </ptl_get_id>
                </ptl_pt>
              </ptl_eq>
            </ptl_ni>
          </ompi_rt>
        </ptl_ni>
      </ptl>
    </test>