<?xml version="1.0"?>
<test>
  <desc>Test put le with ack</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ompi_rt>
        <ptl_eq>
          <ptl_pt>
            <ptl_le_append le_opt="OP_PUT" uid="ANY">
              <ptl_eq>
                <ptl_md>
                  <ptl_put ack_req="ACK" target_id="SELF"/>
                  <ptl_eq_wait eq_handle="eq[0]">
                    <check event_type="LINK"/>
                  </ptl_eq_wait>
                  <ptl_eq_wait eq_handle="eq[0]">
                    <check event_type="PUT"/>
                  </ptl_eq_wait>
                  <ptl_eq_wait eq_handle="eq[1]">
                    <check event_type="SEND"/>
                  </ptl_eq_wait>
                  <ptl_eq_wait eq_handle="eq[1]">
                    <check event_type="ACK"/>
                  </ptl_eq_wait>
                  <msleep time="50"/>
                  <ptl_eq_get eq_handle="eq[0]" ret="EQ_EMPTY"/>
                  <ptl_eq_get eq_handle="eq[1]" ret="EQ_EMPTY"/>
                </ptl_md>
              </ptl_eq>
              <ptl_le_unlink/>
            </ptl_le_append>
          </ptl_pt>
        </ptl_eq>
      </ompi_rt>
    </ptl_ni>
  </ptl>
</test>
